            <section class="signal">
                <div class="signal__stroke">
                    <marquee scrolldelay="1">
                        <span>
                            продвижение за горизонт
                        </span>
                        <span>
                            продвижение за горизонт
                        </span>
                    </marquee>
                </div>
                <div class="signal__container container">
                    <div class="signal__content">
                        <div class="signal__title title">сигнальте</div>
                        <div class="load">
                            <div class="load__title"></div>
                            <div class="load__subtitle"></div>
                            <div class="load__image"></div>
                        </div>
                        <form action="" class="signal__form">
                            <input type="text" placeholder="ФИО" class="signal__name-input">
                            <input type="text" placeholder="Почта / телефон" class="signal__email-input">
                            <div class="signal__textarea-input">
                                <textarea name="" id="" placeholder="Кратко опишите свой проект" maxlength="300"></textarea>
                                <span>... /300</span>
                            </div>
                            <div class="signal__buttons">
                                <span><button class="signal__btn" disabled>Прием!</button></span>
                                <div class="signal__checkboxes">
                                    <div class="signal__agree">
                                        <input type="checkbox" name="checkbox" class="signal__chackbox">
                                        <label class="signal__chackbox-label" for="checkbox">Согласен (-на) на обработку персональных данных</label>
                                    </div>
                                    <div class="signal__callback">
                                        <img src="img/signal/callback.png" alt="callback">
                                        <span>Ответим в течение часа</span>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
